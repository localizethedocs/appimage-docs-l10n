"\n"
"function update() {\n"
"  var variables = {\n"
"    'id': $('#appstream-generator #id').val(),\n"
"    'name': $('#appstream-generator #name').val(),\n"
"    'summary': $('#appstream-generator #summary').val(),\n"
"    'description': $('#appstream-generator #description').val(),\n"
